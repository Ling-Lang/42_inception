server {
    # Listen to http
    listen	80;
    # Listen to https ssl
    listen	443 ssl;
    # Set up the domain
    server_name	jkulka.42.fr www.jkulka.42.fr;
    # Root directory of the project
    root	/var/www/public/html;
    # Path to crt and key
    ssl_certificate	/etc/nginx/ssl/jkulka.42.fr.crt;
    ssl_certificate_key	/etc/nginx/ssl/jkulka.42.fr.key;
    # Supported  tls protocols
    ssl_protocols	TLSv1.2 TLSv1.3;
    # Cache and timeout
    ssl_session_timeout 10m;
    keepalive_timeout 70;
    # Tell the server the ext of the file in the root directory 
    location / {
        try_files $uri /index.html;
    }
}
